<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Movimientos" table="movimientos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="direccion" type="string" column="direccion" length="255" nullable="false"/>
    <field name="periodo" type="datetime" column="periodo" nullable="false"/>
    <field name="grupo" type="string" column="grupo" length="70" nullable="false"/>
    <field name="subgrupo" type="string" column="subgrupo" length="70" nullable="false"/>
    <field name="seccion" type="string" column="seccion" length="70" nullable="false"/>
    <field name="concepto" type="string" column="concepto" length="200" nullable="false"/>
    <field name="cantidad" type="integer" column="cantidad" nullable="false"/>
    <many-to-one field="idEstd" target-entity="Estado">
      <join-columns>
        <join-column name="id_estd" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCord" target-entity="Coordinacion">
      <join-columns>
        <join-column name="id_cord" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDirc" target-entity="Direccion">
      <join-columns>
        <join-column name="id_dirc" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMuni" target-entity="Municipio">
      <join-columns>
        <join-column name="id_muni" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idRspn" target-entity="Reponsable">
      <join-columns>
        <join-column name="id_rspn" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ResumenCuentas" table="resumen_cuentas">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombRcuentas" type="string" column="nomb_rcuentas" length="30" nullable="false"/>
    <many-to-one field="idMuni" target-entity="Municipio">
      <join-columns>
        <join-column name="id_muni" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstd" target-entity="Estado">
      <join-columns>
        <join-column name="id_estd" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
